<h1>All Scientists</h1>

<% @scientists.each do |scientist|%>
<ul>
    <%= link_to scientist.name, scientist_path(scientist) %>
    <%= link_to 'Edit', edit_scientist_path(scientist) %>
    <%= link_to 'Delete', scientist_path(scientist), method: :delete, data: { confirm: 'Are you positive?' } %>
</ul>
<% end %>

<br>
<%= link_to "Add Another Scientist", new_scientist_path %>
<br>
<%= link_to "Start Over", about_path %>